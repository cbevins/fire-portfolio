{
  "name": "fire-portfolio",
  "version": "0.1.0",
  "description": "A browser-based, single page application for creating charts, tables, and diagrams of wildland fire behavior based on the BehavePlus Wildland Fire Simulation System",
  "author": "Collin D. Bevins <cbevins@montana.com>",
  "license": "MIT",
  "keywords": [
    "wildland fire simulation",
    "BehavePlus"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cbevins/fire-portfolio.git"
  },
  "bugs": {
    "url": "https://github.com/cbevins/fire-portfolio/issues"
  },
  "homepage": "https://github.com/cbevins/fire-portfolio#readme",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv public",
    "lint": "eslint \"**/*.js\"",
    "lint:fix": "prettier-eslint \"**/*.js\" --write",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:coveralls": "jest --testTimeout=10000 --runInBand --coverage && coveralls < coverage/lcov.info",
    "test:watch": "jest --watch"
  },
  "devDependencies": {
    "@babel/core": "7.9.6",
    "@babel/preset-env": "7.9.6",
    "@babel/preset-react": "7.9.4",
    "@rollup/plugin-babel": "5.0.0",
    "@rollup/plugin-commonjs": "^12.0.0",
    "@rollup/plugin-json": "4.0.3",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@smui/common": "1.0.0-beta.21",
    "@smui/drawer": "1.0.0-beta.21",
    "@smui/list": "1.0.0-beta.21",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "26.0.1",
    "coveralls": "3.1.0",
    "eslint": "7.0.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-config-standard": "14.1.1",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-jest": "23.10.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.0.1",
    "eslint-plugin-svelte3": "2.7.3",
    "jest": "26.0.1",
    "prettier-eslint": "9.0.1",
    "prettier-eslint-cli": "5.0.0",
    "prettier-plugin-svelte": "1.1.0",
    "rimraf": "3.0.2",
    "rollup": "^2.3.4",
    "rollup-plugin-babel": "4.4.0",
    "rollup-plugin-livereload": "^1.0.0",
    "rollup-plugin-svelte": "^5.0.3",
    "rollup-plugin-terser": "^5.1.2",
    "svelte": "^3.0.0",
    "sveltestrap": "3.3.3"
  },
  "jest": {
    "transform": {
      "^.+\\.[t|j]sx?$": "babel-jest",
      "^.+\\.mjs?$": "babel-jest",
      "^.+\\.js?$": "babel-jest"
    },
    "setupFilesAfterEnv": [
      "./jest.setup.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/archive"
    ],
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/index.js",
      "!**/coverage/**",
      "!**/dist/**",
      "!**/node_modules/**",
      "!**/utils/**",
      "!**/vendor/**",
      "!**/delayed__tests__/",
      "!app.js",
      "!rollup.config.js"
    ]
  },
  "dependencies": {
    "behaveplus-core": "file:../behaveplus-core",
    "bootstrap": "4.5.0",
    "sirv-cli": "^0.4.4",
    "yootils": "0.0.16"
  }
}
